<%# default partial to display solr document fields in catalog show view -%>
<dl class="defList">

  <% Blacklight.config[:show_fields][:field_names].each do |solr_fname| -%>
    <% if document[solr_fname] %>
        <dt><%= Blacklight.config[:show_fields][:labels][solr_fname]%></dt>
        <dd><%= document[solr_fname] %></dd>
    <% end -%>
  <% end -%>

</dl>