<%# default partial to display solr document fields in catalog index view -%>
<dl class="defList">
  
  <% Blacklight.config[:index_fields][:field_names].each do |solr_fname| -%>
    <% if document.get solr_fname %>
        <dt><%= Blacklight.config[:index_fields][:labels][solr_fname]%></dt>
        <dd><%= document.get solr_fname %></dd>
    <% end -%>
  <% end -%>

</dl>